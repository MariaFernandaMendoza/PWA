{
  "name": "my-pwa",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 4173 --force --host 0.0.0.0",
    "dev:clean": "vite --port 4173 --force --clearScreen false",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "serve": "vite preview --host 0.0.0.0 --port 4173",
    "clean": "rm -rf node_modules/.vite dist",
    "start": "npm run build && npm run serve"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.8"
  }
}